{
	"name": "store",
	"version": "0.0.0",
	"description": "A store wechat miniprogram",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"mp:upload": "node scripts/upload-miniprogram.js"
	},
	"engines": {
		"node": ">=16"
	},
	"devDependencies": {
		"turbo": "latest"
	},
	"turbo": {
		"pipeline": {
			"build": {
				"dependsOn": [
					"^build"
				],
				"outputs": [
					"dist/**"
				]
			},
			"lint": {
				"outputs": []
			},
			"dev": {
				"cache": false
			}
		}
	}
}
