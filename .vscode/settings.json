{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.associations": {
    "*.wxss": "css",
    "*.wxml": "wxml",
    "*.wxs": "javascript"
  },
  "windicss.sortOnSave": true,
  "windicss.includeLanguages": {
    "*.wxml": "html"
  },
  "eslint.workingDirectories": [{"mode": "auto"}]
}
